<?xml version="1.0" encoding="UTF-8"?>
<configs>
	<config name="name" value="IpuServer"/>
	<config name="productMode" value="false"/>
	<config name="productType" value="boot"/>

	<!-- 服务端资源版本号 -->
	<config name="resourceVersion" value="1.0"/>
	<!-- 客户端版本号 -->
	<config name="clientVersion" value="1.0"/>
	<!-- 客户端是否强制更新 -->
	<config name="isForceUpdate" value="true"/>
	<!-- 服务端应用编码 -->
	<config name="encode" value="UTF-8"/>
	<!-- 缓存类型,目前支持mem和jvm -->
	<config name="cache" value="jvm"/>
	<!-- 初始页 -->
	<config name="indexPage" value="Login"/>
	<!-- 文件是否加密 -->
	<config name="fileEncrypt" value="true"/>
	<!-- 自定义异常管理器 -->
	<config name="exceptionHandler" value="com.ai.interfacetest.core.handler.IpuExceptionHandler"/>
	<!-- 自定义Session管理器 -->
	<config name="sessionManager" value="com.ai.interfacetest.core.session.IpuSessionManager"/>
	<!-- 自定义安全管理器 -->
	<config name="securityHandler" value="com.ai.interfacetest.core.handler.IpuSecurityHandler"/>
</configs>





